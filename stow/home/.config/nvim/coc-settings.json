{
  "languageserver": {
    "clojure-lsp": {
      "command": "bash",
      "args": [ "-c", "clojure-lsp" ],
      "filetypes": [ "clojure" ],
      "rootPatterns": [
        "project.clj",
        "build.boot",
        "deps.edn"
      ],
      "additionalSchemes": [
        "jar",
        "zipfile"
      ],
      "trace.server": "verbose",
      "initializationOptions": {}
    }
  }
}
